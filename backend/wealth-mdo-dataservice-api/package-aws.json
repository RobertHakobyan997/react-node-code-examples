{
  "name": "ngpd-merceros-wealth-mdo-dataservice-api",
  "version": "1.3.1",
  "description": "Data API service",
  "author": "Wealth MDO",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rimraf ./node_modules",
    "format": "npm run lint -- --fix",
    "build": "nest build",
    "ci": "npm run lint && npm test && npm run build",
    "build:prod": "npm run build",
    "assemble": "tsc -p tsconfig.build.json && cp package.json dist/package.json && cd dist/ && npm install --prod --no-color",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env TS_NODE_PROJECT=./tsconfig.build.json node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --passWithNoTests --detectOpenHandles --colors --coverage"
  },
  "dependencies": {
    "@nestjs/common": "8.2.1",
    "@nestjs/config": "1.1.0",
    "@nestjs/core": "8.2.1",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/mapped-types": "1.0.0",
    "@nestjs/microservices": "8.2.1",
    "@nestjs/passport": "8.0.1",
    "@nestjs/platform-express": "8.2.1",
    "@nestjs/swagger": "5.1.4",
    "@nestjs/websockets": "8.2.1",
    "body-parser": "1.19.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "clone-deep": "4.0.1",
    "cookie-parser": "1.4.6",
    "cross-env": "7.0.3",
    "csurf": "1.11.0",
    "dd-trace": "1.5.1",
    "dotenv": "10.0.0",
    "getenv": "1.0.0",
    "http-proxy-middleware": "2.0.1",
    "jszip": "3.7.1",
    "lodash": "4.17.21",
    "luxon": "2.1.1",
    "migrate-mongo": "8.2.3",
    "mongodb": "4.1.4",
    "mongoose": "6.0.13",
    "mongoose-aggregate-paginate-v2": "1.0.42",
    "mongoose-paginate": "5.0.3",
    "mongoose-paginate-v2": "1.4.2",
    "ngpd-merceros-logger-be": "1.1.10",
    "ngpd-merceros-wealth-mdo-common-be": "1.7.23",
    "passport": "0.5.0",
    "reflect-metadata": "0.1.13",
    "require-glob": "4.0.0",
    "rxjs": "7.4.0",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@nestjs/cli": "8.1.4",
    "@nestjs/testing": "8.2.1",
    "@types/express": "4.17.13",
    "@types/faker": "5.5.9",
    "@types/getenv": "1.0.0",
    "@types/jest": "27.0.2",
    "@types/jsonwebtoken": "8.5.6",
    "@types/lodash": "4.14.177",
    "@types/luxon": "2.0.7",
    "@types/mongoose-aggregate-paginate-v2": "1.0.3",
    "@types/mongoose-paginate-v2": "1.4.0",
    "@types/multer": "1.4.7",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.4.0",
    "@typescript-eslint/parser": "5.4.0",
    "boolean": "3.1.4",
    "concurrently": "6.4.0",
    "dotenv-defaults": "3.0.0",
    "eslint": "8.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-unicorn": "38.0.1",
    "faker": "5.5.3",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "jest-cli": "27.3.1",
    "jest-sonar-reporter": "2.0.0",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4",
    "typescript-mix": "3.1.3",
    "wait-on": "6.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format",
      "pre-push": "npm run ci"
    }
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      ".spec-setup.ts",
      "index.ts",
      "constants.ts",
      "const.ts",
      "consts.ts",
      "utils.ts",
      "provider.ts",
      "type.ts",
      "interface.ts",
      "model.ts",
      "enum.ts",
      "query.ts",
      "main.ts",
      ".decorator.ts",
      ".class.ts",
      ".factory.ts",
      ".module.ts",
      ".controller.ts",
      ".dal.ts",
      "dev-logger.ts",
      "schema.ts",
      "dto.ts",
      "environment.utils.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/dist/**",
      "!**/src/dev/**",
      "!**/src/config/**",
      "!**/src/decorators/**",
      "!**/src/user/**",
      "!**/src/main.ts",
      "!**/src/app.module.ts",
      "!**/src/app-imports-loader.ts",
      "!**/src/utilities",
      "!**/src/example/**"
    ],
    "setupFiles": ["<rootDir>/setup-tests.js"],
    "globalTeardown": "<rootDir>/teardown.js",
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter",
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/tsconfig.spec.json"
      }
    }
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
