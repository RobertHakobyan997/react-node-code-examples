{
  "verbose": true,
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/",
    "dist/",
    "coverage/",
    "src/**/*.spec.ts"
  ],
  "watch": [
    "src/"
  ],
  "exec": "./node_modules/.bin/ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts",
  "env": {
    "NODE_ENV": "dev"
  },
  "ext": "js,json,ts"
}
