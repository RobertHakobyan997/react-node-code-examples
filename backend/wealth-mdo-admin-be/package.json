{
  "name": "ngpd-merceros-wealth-mdo-admin-be",
  "version": "1.2.1",
  "description": "MDO Core Backend",
  "author": "Elena Pronina",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "clean": "rimraf ./node_modules",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "ci": "npm run lint && npm test && npm run build",
    "build:prod": "rimraf ./dist && tsc -p tsconfig.build.json",
    "assemble": "tsc -p tsconfig.build.json && cp package.json dist/package.json && cd dist/ && npm install --prod --no-color",
    "start": "ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts",
    "start:docker": "npm run local-migration && npm run start",
    "start:dev": "nodemon",
    "start:prod": "TS_NODE_PROJECT=./tsconfig.build.json node -r dotenv/config -r tsconfig-paths/register dist/src/main.js",
    "start:debug": "nodemon --config nodemon-debug.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest --passWithNoTests --detectOpenHandles --colors --coverage",
    "migrate": "migrate-mongo up -f \"migrations/configs/migrate-mongo-config.js\"",
    "migrate-auth-local": "migrate-mongo up -f \"local-migrations/configs/migrate-mongo-authorization-api-local-config.js\"",
    "migrate:docker": "migrate-mongo up -f \"migrations/configs/migrate-mongo-config-docker.js\"",
    "migrate-auth-local:docker": "migrate-mongo up -f \"local-migrations/configs/migrate-mongo-authorization-api-local-config-docker.js\"",
    "local-migration": "npm run migrate:docker && npm run migrate-auth-local:docker",
    "prepare:ta": "npm install --legacy-peer-deps",
    "refresh:ta": "npm update --legacy-peer-deps && npm update ngpd-merceros-wealth-mdo-common-be && npm update ngpd-merceros-wealth-mdo-common-fe",
    "build:ta": "tsc -p tsconfig.build.json && npm run migrate && npm run migrate-auth-local",
    "start:ta": "npm start"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run ci"
    }
  },
  "dependencies": {
    "@hapi/joi": "16.1.8",
    "@nestjs/common": "7.6.13",
    "@nestjs/core": "7.6.13",
    "@nestjs/event-emitter": "1.0.0",
    "@nestjs/jwt": "7.2.0",
    "@nestjs/mapped-types": "1.0.0",
    "@nestjs/microservices": "7.6.13",
    "@nestjs/mongoose": "7.2.4",
    "@nestjs/passport": "7.1.5",
    "@nestjs/platform-express": "7.6.13",
    "@nestjs/schedule": "1.0.1",
    "@nestjs/swagger": "4.8.2",
    "@nestjs/websockets": "7.6.13",
    "body-parser": "1.19.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.13.0",
    "clone-deep": "4.0.1",
    "cookie-parser": "1.4.5",
    "csurf": "1.11.0",
    "dd-trace": "0.21.1",
    "dotenv": "8.2.0",
    "getenv": "1.0.0",
    "http-proxy-middleware": "1.0.6",
    "lodash": "4.17.21",
    "luxon": "1.26.0",
    "migrate-mongo": "8.1.4",
    "mongodb": "4.1.3",
    "mongoose": "5.13.13",
    "mongoose-aggregate-paginate-v2": "1.0.42",
    "mongoose-paginate-v2": "1.4.2",
    "ngpd-merceros-authentication-be-components": "bitbucket:oliverwymantechssg/ngpd-merceros-authentication-be-components#v2.4.2",
    "ngpd-merceros-authorization-client-be": "git+ssh://git@bitbucket.org/oliverwymantechssg/ngpd-merceros-authorization-client-be.git",
    "ngpd-merceros-communication-adapter": "git+ssh://git@bitbucket.org/oliverwymantechssg/ngpd-merceros-communication-adapter.git",
    "ngpd-merceros-dataaccess-components": "git+ssh://git@bitbucket.org/oliverwymantechssg/ngpd-merceros-dataaccess-components.git",
    "ngpd-merceros-documents-adapter": "git+ssh://bitbucket.org/oliverwymantechssg/ngpd-merceros-documents-adapter.git#6707be87c77dc35851dbad681a73b1bafc8bcb9b",
    "ngpd-merceros-logger-be": "git+ssh://git@bitbucket.org/oliverwymantechssg/ngpd-merceros-logger-be.git",
    "ngpd-merceros-wealth-mdo-common-be": "bitbucket:oliverwymantechssg/wealth-mdo-common-be",
    "ngpd-merceros-wealth-mdo-common-fe": "bitbucket:oliverwymantechssg/wealth-mdo-common-fe",
    "passport": "0.4.1",
    "reflect-metadata": "0.1.13",
    "require-glob": "4.0.0",
    "rxjs": "6.6.6",
    "swagger-ui-express": "4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-angular": "8.3.4",
    "@nestjs/cli": "7.5.6",
    "@nestjs/testing": "7.5.1",
    "@types/cron": "1.7.3",
    "@types/express": "4.17.11",
    "@types/faker": "5.1.2",
    "@types/getenv": "1.0.0",
    "@types/jest": "27.0.0",
    "@types/joi": "14.3.4",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.168",
    "@types/luxon": "1.26.4",
    "@types/mongoose-aggregate-paginate-v2": "1.0.1",
    "@types/mongoose-paginate-v2": "1.3.6",
    "@types/node": "14.14.35",
    "@types/passport": "1.0.6",
    "@types/pg": "7.14.11",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "boolean": "3.0.2",
    "concurrently": "6.2.0",
    "eslint": "7.22.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-unicorn": "12.1.0",
    "faker": "5.3.1",
    "husky": "5.2.0",
    "jest": "27.1.0",
    "jest-cli": "27.1.0",
    "jest-sonar-reporter": "2.0.0",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "ts-jest": "27.0.5",
    "ts-loader": "8.0.8",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.2.3",
    "typescript-mix": "3.1.3",
    "wait-on": "3.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
      ".wrapper.ts",
      ".spec-setup.ts",
      "index.ts",
      "constants.ts",
      "const.ts",
      "provider.ts",
      "type.ts",
      "interface.ts",
      "model.ts",
      "enum.ts",
      "query.ts",
      "main.ts",
      ".decorator.ts",
      ".class.ts",
      ".factory.ts",
      ".module.ts",
      ".controller.ts",
      "dev-logger.ts",
      "schema.ts",
      "dto.ts",
      "environment.utils.ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/dist/**",
      "!**/src/**/*.spec-setup.ts",
      "!**/src/**/*.{consts,utils}.ts",
      "!**/src/config/**",
      "!**/src/user/**",
      "!**/src/main.ts",
      "!**/src/app.module.ts",
      "!**/src/app-imports-loader.ts",
      "!**/src/reverse-proxy.middleware.ts",
      "!**/src/util.ts",
      "!**/src/example/**"
    ],
    "globalTeardown": "<rootDir>/teardown.js",
    "testEnvironment": "node",
    "testResultsProcessor": "jest-sonar-reporter",
    "preset": "ts-jest",
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json"
      }
    }
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
